@model CommonJavaScriptViewModel

@Narvalo.Web.Html.AssetTag.JavaScript("app-" + Model.Version + ".min.js")

<script>
 (function(a) {
  if (!a){return;}(new a({version:'@Model.Version'})).main('@Model.LanguageName',function(_){_.handle('@Model.ControllerName','@Model.ActionName');});
 })(this.App);
</script>
