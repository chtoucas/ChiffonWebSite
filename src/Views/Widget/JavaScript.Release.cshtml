@model CommonJavaScriptViewModel

@Narvalo.Web.Html.AssetTag.JavaScript("main-" + Model.Version + ".min.js")

<script>
 (function(m) {
  if (!m) { return; }
  Bundles.version = '@Model.Version';
  main('@Model.LanguageName', function(_) { _.handle('@Model.ControllerName', '@Model.ActionName'); });
 })(this.main);
</script>
