@using Narvalo

@model CommonJavaScriptViewModel

@Narvalo.Web.Html.AssetTag.JavaScript("app-" + Model.Version + ".min.js")

<script>(function(a,o){if(!a){return;}o={baseUrl:'@Model.BaseUrl.ToProtocolLessString()',version:'@Model.Version'};(new a(o)).main('@Model.LanguageName',function(_){_.handle('@Model.ControllerName','@Model.ActionName');});})(this.App);</script>
