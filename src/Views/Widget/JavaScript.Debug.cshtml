@model CommonJavaScriptViewModel

@Narvalo.Web.Html.AssetTag.JavaScript("vendor/yepnope-1.5.4.min.js")
@Narvalo.Web.Html.AssetTag.JavaScript("vendor/ecma-5.js")
@Narvalo.Web.Html.AssetTag.JavaScript("main.js")

<script>
 (function(yepnope, Env, main, undef) {
  if (undef === yepnope || undef === main) { return; }
  Env.Init('@(Model.Version)', @(Model.IsAuthenticated), '@(Model.LanguageName)', true /* debug */, '/assets/js/');
  main(function(chiffon) { chiffon.handle('@Model.ControllerName', '@Model.ActionName'); });
 })(this.yepnope, this.Env, this.main);
</script>
