@model CommonJavaScriptViewModel

@Narvalo.Web.Html.AssetTag.JavaScript("vendor/yepnope-1.5.4.min.js")
@Narvalo.Web.Html.AssetTag.JavaScript("vendor/lodash.compat-2.0.0.min.js")
@Narvalo.Web.Html.AssetTag.JavaScript("app.js")

<script>
 (function(App) {
  if (!App) { return; }
  var options = { baseUrl: '@Model.ScriptBase', debug: true };
  (new App(options)).main('@Model.LanguageName', function(chiffon) { chiffon.handle('@Model.ControllerName', '@Model.ActionName'); });
 })(this.App);
</script>
