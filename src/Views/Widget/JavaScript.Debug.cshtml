@model CommonJavaScriptViewModel

@Narvalo.Web.Html.AssetTag.JavaScript("vendor/yepnope-1.5.4.min.js")
@Narvalo.Web.Html.AssetTag.JavaScript("vendor/lodash.compat-1.3.1.min.js")
@Narvalo.Web.Html.AssetTag.JavaScript("main.js")

<script>
 (function(main) {
  if (!main) { return; }
  main('@Model.LanguageName', function(chiffon) { chiffon.handle('@Model.ControllerName', '@Model.ActionName'); });
 })(this.main);
</script>
