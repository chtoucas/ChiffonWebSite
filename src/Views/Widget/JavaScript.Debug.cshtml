@model CommonJavaScriptViewModel

@Narvalo.Web.Html.AssetTag.JavaScript("vendor/yepnope-1.5.4.min.js")
@Narvalo.Web.Html.AssetTag.JavaScript("vendor/lodash.compat-1.3.1.min.js")
@Narvalo.Web.Html.AssetTag.JavaScript("main.js")

<script>
 (function(yepnope, Env, main, undef) {
  if (undef === yepnope || undef === main) { return; }
  /* @Model.IsAuthenticated, */
  var config = new Config('@Model.Version', '@Model.LanguageName', true /* debug */, '/assets/js/');
  main(config, function(chiffon) { chiffon.handle('@Model.ControllerName', '@Model.ActionName'); });
 })(this.yepnope, this.Env, this.main);
</script>
