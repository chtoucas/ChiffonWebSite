@using Narvalo.Web.Html

@model CommonJavaScriptViewModel

@AssetTag.JavaScript("app-" + Model.Version + ".min.js")

<script>(function(a,o){if(!a){return;}o={baseUrl:'@Model.ScriptBase',version:'@Model.Version'};(new a(o)).main(@(Model.IsAuthenticated.ToString().ToLower()),'@Model.LanguageName',function(_){_.handle('@Model.ControllerName','@Model.ActionName');});})(this.App);</script>
