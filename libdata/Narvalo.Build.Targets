<?xml version="1.0" encoding="utf-8" ?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <TasksPath>$(MSBuildProjectDirectory)\libexec</TasksPath>
    <TasksLib>$(WibilongTasksPath)\Narvalo.Build.dll</TasksLib>
  </PropertyGroup>

  <UsingTask TaskName="Narvalo.Build.ClosureCompiler" AssemblyFile="$(TasksLib)" />
